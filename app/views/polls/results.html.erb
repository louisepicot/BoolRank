
<div class="results-container flex-box ">
  <div class="navbar-box">
    <div class="pull-left">
      <%= link_to '< Back', add_propositions_poll_path(@poll)%>
    </div>
    <div class="pull-right">
      <%= link_to 'home / ', root_path%>
    </div>
  </div>


  <%= link_to root_path do %>

  <% end %>

  <a href="#participants">
    <div class="btn-green-participants">
      <span class="btn-square">
        <span class="btn__inner"><i class="fa fa-users" aria-hidden="true"></i></span>
      </span>
    </div>
  </a>

  <a href="#propositions">
    <div class="btn-red-propositions">
      <span class="btn-square">
        <span class="btn__inner"><i class="fa fa-linode" aria-hidden="true"></i></span>
      </span>
    </div>
  </a>


  <a href="#share">
    <div class="btn-pink-share">
      <span class="btn-square">
        <span class="btn__inner"><i class="fa fa-share" aria-hidden="true"></i></span>
      </span>
    </div>
  </a>




  <div class="prop-container background-gif margin-b-results">
    <div class="btn-location prop-title">
      <h3 class="bold text-uppercase text-center">
        <%#= @propositions.first.name %>
        Your <br> choice
      </h3>
    </div>
    <div class="prop-score text-center text-uppercase">
      <i class="fa fa-star" aria-hidden="true"><h2>score: <%= @propositions.first.score %></h2></i>
    </div>
    <% if @propositions.first.photo.nil? %>
    <div class="prop-image-first" style="background-color:  <%= @propositions.first.color %>">
    </div>
    <% else %>
    <div class="prop-image-first text-center" style="background-color: #060303;">
     <%= cl_image_tag @propositions.first.photo.path %>
    </div>
   <% end %>
 </div>


 <div class="prop-container background-gif margin-b-results" id="propositions">
   <div class="btn-location-blue prop-title">
    <h3 class="bold text-uppercase text-center">
     All propositions
   </h3>
 </div>
 <% @propositions[1..-1].each do |proposition| %>
 <div class="row">
  <div class="col-xs-12">
    <div class="prop-box">
      <div class="photo-box">
        <% unless proposition.photo.nil?  %>
        <div class="created-photo" style="background-image: url('<%= cl_image_path  proposition.photo.path %>')">
        </div>
        <% else %>
        <div class="created-photo" style="background-color: <%=proposition.color%>">
        </div>
        <% end %>
      </div>
      <div class="name-box">
        <div class="created-name">
          <h3><%= proposition.name %></h3>
        </div>
      </div>
    </div>
  </div>
</div>
<% end %>
</div>



<div class="prop-container background-gif margin-b-results" id="participants">
  <div class="btn-location-green prop-title">
   <h3 class="bold text-uppercase text-center">
     Participants
   </h3>
 </div>

 <% @propositions[1..-1].each do |proposition| %>
 <div class="row">
  <div class="col-xs-12">
    <div class="prop-box">
      <div class="photo-box">
        <% unless proposition.photo.nil?  %>
        <div class="created-part-photo" style="background-image: url('<%= image_path  "Bear.jpg" %>')"></div>
        <% else %>
        <div class="created-part-photo" style="background-color: <%=proposition.color%>"></div>
        <% end %>
      </div>
      <div class="name-box">
        <div class="created-part-name">
          <h3><%= proposition.name %></h3>
        </div>
      </div>
    </div>
  </div>
</div>
<% end %>
</div>



<button type="button" class="btn btn-default btn-medium margin-b-results" data-toggle="modal" data-target="#share-modal" id="share">
  Share Poll!
</button>




</div>




